{
  "name": "brainrobot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "bin": "dist/src/server.js",
  "pkg": {
    "assets": [
      "dist/**/*",
      "dist/**/*.json",
      "dist/**/*.html",
      "node_modules/@whiskeysockets/baileys/node_modules/axios/dist/**/*",
      "package.json",
      "node_modules/@peculiar/webcrypto/build/**/*",
      "baileys_auth/**/*"
    ],
    "scripts": [
      "node_modules/@whiskeysockets/baileys/lib/**/*.js",
      "dist/**/*.js"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "pkg-build"
  },
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:prod": "node dist/src/server.js",
    "start": "nodemon --watch \"src/**/*.ts\" -e ts,json --exec \"ts-node --transpile-only ./src/server.ts\"",
    "pkg": "npm run build && pkg . --compress Brotli --targets node18-win-x64 --output ../backend.exe"
  },
